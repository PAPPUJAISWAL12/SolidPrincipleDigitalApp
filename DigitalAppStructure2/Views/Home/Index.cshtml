@model IEnumerable<UserListEdit>

<div class="mb-2">
    <a asp-action="Create" asp-controller="Home" class="text-white py-2.5 px-5 rounded-lg bg-blue-600 hover:bg-blue-800 focus:ring-blue-300 focus:ountline-none focus:ring">Add New User</a>

</div>
<div class="flex flex-wrap gap-2 mt-5">
    @if (Model.Count() > 0 || Model!=null)
    {
        foreach (var i in Model)
        {


            <div class="w-full max-w-sm bg-white border border-gray-200 rounded-lg shadow dark:bg-gray-800 dark:border-gray-700">
               
                <div class="flex flex-col items-center p-8">
                    <img class="w-24 h-24 mb-3 rounded-full shadow-lg" src="~/UserProfile/@i.UserProfile" alt="Bonnie image" />
                    <h5 class="mb-1 text-xl font-medium text-gray-900 dark:text-white">@i.UserName</h5>
                    <span class="text-sm text-gray-500 dark:text-gray-400">@i.UserAddress</span>
                    <div class="flex mt-4 md:mt-6">
                       <a asp-action="Details" asp-route-id="@i.EncId" class="text-white bg-blue-700 hover:bg-blue-800 py-3 px-5 rounded-lg">Details</a>
                        <a asp-action="Edit" asp-route-id="@i.EncId" class="text-white bg-green-700 hover:bg-green-800 py-3 px-5 rounded-lg">Edit</a>
                    </div>
                </div>
            </div>

        }
        
    }
    else
    {
        <div class="text-gray-800" >
            <p >Data not found</p>
        </div>
        
    }
</div>